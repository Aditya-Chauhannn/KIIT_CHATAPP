<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!-- Background color for your logo, change as needed -->
    <solid android:color="#FF000000"/> <!-- Black, you can use @color/your_color -->
    <!-- Corner radius -->
    <corners android:radius="10dp"/>  <!-- Change this value to set your desired roundness -->
    <!-- Optional: Add a border -->
    <stroke android:width="9dp" android:color="#55171616"/>
    <!-- Optional: Add padding inside the shape -->
    <padding android:left="12dp" android:top="12dp" android:right="12dp" android:bottom="12dp"/>
</shape>